<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="conequate_googleapis_script" type="host">https://maps.googleapis.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="conequate_googleapis_connect" type="host">https://maps.googleapis.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="conequate_gstatic_img" type="host">https://maps.gstatic.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
